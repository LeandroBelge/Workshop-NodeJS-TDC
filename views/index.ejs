
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% docs.forEach(function(customer){ %>
      <li>
        <a href="/edit/<%= customer._id %>">
          <%= customer.nome %>
        </a>
        <a href="/delete/<%= customer._id %>" onclick="return confirm('Tem certeza que deseja excluir')">
          x
        </a>
      </li>
      <% })%>
    </ul>
  </body>
</html>